#include <iostream>
using namespace std;

class complex{
  int a,b;
  public:
  void setdata(int , int );
  void showdata()
  {
      cout<<ab;
      
  }
};
void complex::setdata(int x,int y)
{
    a=x;
    b=y;
}

int main() {
	// your code goes here
	complex c1;
	c1.setdata(3,4);
	c1.showdata();
	return 0;
}
